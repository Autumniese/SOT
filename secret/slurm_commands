Run container:
srun -w dgx03 -G 1 --ntasks 1 --cpus-per-task 32 --container-image=/users/pilot/tester3/Daniel/SOT.sqsh --pty /bin/bash
cd
cd Daniel/SOT

Train:
python train.py --project FS_minimagenet --entity danielsha --data_path /root/Daniel/datasets/MiniImagenet/ --pretrained_path ./checkpoints/wrn/miniimagenet/WideResNet28_10_S2M2_R/470.tar --wandb true --train_way 10 --scheduler step --step_size 10 --lr 2e-5 --log_step false --augment false

Save features:
python methods/PT_MAP/evaluation/save_plk.py --model_dir /root/Daniel/SOT/checkpoints/wrn/miniimagenet/way5_shot5_mask_diagTrue_lr2e-06/ --file_name checkpoint.pth --json_dir /root/Daniel/datasets/MiniImagenet/

PT_MAP Eval:
python methods/PT_MAP/evaluation/test_standard.py --root /root/Daniel/SOT/ --path ./checkpoints/wrn/miniimagenet/way5_shot5_mask_diagTrue_lr2e-06/last/output.plk

python methods/PT_MAP/evaluation/test_standard.py --root /root/Daniel/SOT/ --path ./checkpoints/wrn/miniimagenet/WideResNet28_10_S2M2_R/last/output.plk

Eval:
python eval.py --data_path /root/Daniel/datasets/MiniImagenet/ --pretrained_path ./checkpoints/wrn/miniimagenet/way5_shot5_mask_diagTrue/checkpoint.pth --wandb true --project FS_minimagenet --entity danielsha --log_step false
